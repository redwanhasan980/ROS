<?xml version="1.0"?>
<launch>
  <param name="robot_description" command="xacro '$(find mobile_bot)/urdf/robot.urdf.xacro'"/>
 
    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher" />
    <node pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" name="joint_state_publisher"/>
     <include file="$(find gazebo_ros)/launch/empty_world.launch">
      <!--<arg name="world_name" value="$(find turtlebot3_gazebo)/worlds/empty.world"/>-->
      <arg name="paused" value="false"/>
      <arg name="use_sim_time" value="true"/>
      <arg name="gui" value="true"/>
      <arg name="headless" value="false"/>
      <arg name="debug" value="false"/>
      
     </include>
    <node name="spawining _urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -model robot -param robot_description">
    
    </node>
  
</launch>